<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spongzi.user.mapper.UserMapper">

    <select id="getUserPage" resultType="com.spongzi.user.entity.po.UserPo">
        select id, name, age
        from user
        where delete_flag = 0
    </select>
</mapper>